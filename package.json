{
  "name": "vue-template",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --port 3345 --open",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "postinstall": "npx simple-git-hooks"
  },
  "dependencies": {
    "@vueuse/core": "^10.3.0",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@dcwjoy/eslint-config": "^0.39.30",
    "@types/node": "^20.4.8",
    "@vitejs/plugin-vue": "^4.2.3",
    "add": "^2.0.6",
    "eslint": "^8.46.0",
    "esno": "^0.17.0",
    "lint-staged": "^13.2.3",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.0.2",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.4.5",
    "vite-plugin-vue-devtools": "1.0.0-beta.6",
    "vue-tsc": "^1.8.5"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
